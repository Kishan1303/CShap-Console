using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication2
{
    
    class one
    {
    
        public void sum(int a , int b)
        {
            Console.WriteLine(a+b);
        }
        public void sum(int a)
        {
            
            Console.WriteLine(a);
        }
        
    }
    class Program
    {
        static void Main(string[] args)
        {
            one o = new one();
            o.sum(10);
            o.sum(10, 30);
            Console.Read();
            
        }
    }
}
